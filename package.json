{
  "name": "@codestates-engineering/codeship-platform-app-library",
  "version": "0.3.43",
  "packageManager": "pnpm@8.6.3",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/types.d.ts",
  "repository": "https://github.com/CodeStates-Engineering/codeship-platform-app-library.git",
  "author": "HyeokjaeLee <leehyeokjae97@gmail.com>",
  "homepage": "https://CodeStates-Engineering.github.io/codeship-platform-app-library",
  "scripts": {
    "version": "npm version",
    "version:canary": "sh ./scripts/set-canary-version.sh",
    "dev": "storybook dev -p 6006",
    "clean:build": "rm -rf dist",
    "build:lib": "tsc && vite build",
    "build:doc": "storybook build -o dist/document && touch ./dist/document/.nojekyll",
    "deploy:lib": "npm publish ./dist/library --access=public",
    "deploy:lib-canary": "pnpm run deploy:lib --tag canary",
    "deploy:doc": "gh-pages -d dist/document -t true",
    "rule:eslint": "eslint 'src/**/*.{js,ts,tsx}'",
    "rule:eslint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "rule:prettier": "prettier 'src/**/*.{json,yaml,md,js,ts,tsx,css,scss}' --check",
    "rule:prettier:fix": "prettier 'src/**/*.{json,yaml,md,js,ts,tsx,css,scss}' --write",
    "rule:stylelint": "stylelint './src/**/*.{scss,css}'",
    "rule:stylelint:fix": "stylelint './src/**/*.{scss,css}' --fix",
    "lint:all": "pnpm rule:eslint && pnpm rule:prettier && pnpm rule:stylelint",
    "lint:all:fix": "pnpm rule:eslint:fix && pnpm rule:prettier:fix && pnpm rule:stylelint:fix"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@codestates-engineering/eslint-config-codestates": "1.0.6",
    "@hyeokjaelee/stylelint-config": "^1.3.1",
    "@storybook/addon-actions": "^7.1.1",
    "@storybook/addon-docs": "^7.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/cli": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "@types/rollup-plugin-generate-package-json": "^3.2.0",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.39.0",
    "gh-pages": "^5.0.0",
    "lodash-es": "^4.17.21",
    "next": "^13.2.4",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.11.1",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "sass": "^1.62.1",
    "storybook": "^7.1.1",
    "stylelint": "^15.6.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-static-copy": "^0.14.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "react-day-picker": "^8.7.1",
    "react-node-to-string": "^0.1.1"
  }
}
