{
  "name": "@codestates-engineering/jobs-components",
  "version": "0.0.9",
  "types": "dist/package/types/index.d.ts",
  "module": "dist/package/index.js",
  "repository": "https://github.com/CodeStates-Engineering/jobs-components.git",
  "author": "HyeokjaeLee <leehyeokjae97@gmail.com>",
  "homepage": "https://CodeStates-Engineering.github.io/jobs-components",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "install:dev": "yarn install && yarn dlx @yarnpkg/sdks vscode",
    "version": "npm version",
    "version:canary": "sh ./scripts/set-canary-version.sh",
    "start": "start-storybook --no-manager-cache -p 6006 -s public",
    "clean:build": "rm -rf dist",
    "build:package": "rollup -c",
    "build:storybook": "build-storybook -o dist/storybook",
    "deploy:storybook": "gh-pages -d dist/storybook",
    "deploy:package": "npm publish ./dist/package",
    "deploy:package:canary": "yarn run deploy:package --tag canary",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{json,yaml,md,js,ts,tsx}' --check",
    "prettier:fix": "prettier '**/*.{json,yaml,md,js,ts,tsx}' --write",
    "lint:style": "stylelint './**/*.{js,ts,tsx}'",
    "lint:style:fix": "stylelint './**/*.{js,ts,tsx}' --fix",
    "lint:all": "yarn lint && yarn prettier && yarn lint:style",
    "lint:all:fix": "yarn lint:fix && yarn prettier:fix && yarn lint:style:fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@codestates-engineering/eslint-config-codestates": "1.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/rollup": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "date-fns": "^2.29.3",
    "gh-pages": "^5.0.0",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.5.1",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "rollup": "^3.12.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url": "^3.0.1",
    "sass": "^1.58.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.1.1",
    "webpack": "^5.75.0"
  },
  "optionalDependencies": {
    "@stylelint/postcss-css-in-js": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "postcss-syntax": "*",
    "prettier": "*",
    "stylelint": "*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "packageManager": "yarn@3.3.1"
}
